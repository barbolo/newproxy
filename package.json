{
  "name": "newproxy",
  "version": "1.5.1",
  "description": "",
  "keywords": [],
  "main": "dist/lib/new-proxy.js",
  "module": "dist/lib/new-proxy.js",
  "typings": "dist/types/new-proxy.d.ts",
  "files": [
    "dist"
  ],
  "author": "sannysoft <sannysoft@gmail.com>",
  "repository": {
    "type": "git",
    "url": ""
  },
  "license": "MIT",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "lint": "eslint --cache --ext .js,.jsx,.ts,.tsx ./src",
    "prebuild": "rimraf dist",
    "build": "tsc --module commonjs",
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watch",
    "test:prod": "npm run lint && npm run test -- --no-cache",
    "precommit": "lint-staged",
    "types": "dts-bundle --name newproxy --main dist/types/new-proxy.d.ts --removeSource"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    },
    "collectCoverageFrom": [
      "src/*.{js,ts}"
    ]
  },
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/jest": "^26.0.9",
    "@types/mkdirp": "^1.0.1",
    "@types/node": "^14.0.27",
    "@types/node-forge": "^0.9.4",
    "@typescript-eslint/eslint-plugin": "^3.9.0",
    "@typescript-eslint/parser": "^3.9.0",
    "dts-bundle": "^0.7.3",
    "eslint": "^7.6.0",
    "eslint-config-airbnb-typescript": "^9.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.5",
    "eslint-plugin-unicorn": "^21.0.0",
    "husky": "^4.2.5",
    "import-sort-parser-babylon": "^6.0.0",
    "import-sort-parser-typescript": "^6.0.0",
    "import-sort-style-module": "^6.0.0",
    "jest": "^26.3.0",
    "jest-config": "^26.3.0",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.4",
    "ts-jest": "^26.2.0",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^9.0.0",
    "typedoc": "^0.18.0",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "@postman/tunnel-agent": "github:postmanlabs/tunnel-agent",
    "agentkeepalive": "^4.1.3",
    "chalk": "^4.1.0",
    "debug": "^4.1.1",
    "mkdirp": "^1.0.4",
    "node-cache": "^5.1.2",
    "node-forge": "^0.9.1",
    "ts-hashcode": "^1.0.2"
  }
}
