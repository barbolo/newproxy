{
  "name": "newproxy",
  "version": "1.5.5",
  "description": "",
  "keywords": [],
  "main": "dist/lib/new-proxy.js",
  "module": "dist/lib/new-proxy.js",
  "typings": "dist/types/new-proxy.d.ts",
  "files": [
    "dist"
  ],
  "author": "sannysoft <sannysoft@gmail.com>",
  "repository": {
    "type": "git",
    "url": ""
  },
  "license": "MIT",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "lint": "eslint --cache --ext .js,.jsx,.ts,.tsx ./src",
    "prebuild": "rimraf dist",
    "build": "tsc --module commonjs",
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watch",
    "test:prod": "npm run lint && npm run test -- --no-cache",
    "precommit": "lint-staged",
    "types": "dts-bundle --name newproxy --main dist/types/new-proxy.d.ts --removeSource"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    },
    "collectCoverageFrom": [
      "src/*.{js,ts}"
    ]
  },
  "devDependencies": {
    "@types/debug": "^4.1.6",
    "@types/jest": "^26.0.23",
    "@types/mkdirp": "^1.0.1",
    "@types/node": "^16.0.0",
    "@types/node-forge": "^0.10.0",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.1",
    "dts-bundle": "^0.7.3",
    "eslint": "^7.30.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-unicorn": "^34.0.1",
    "husky": "^7.0.0",
    "import-sort-parser-babylon": "^6.0.0",
    "import-sort-parser-typescript": "^6.0.0",
    "import-sort-style-module": "^6.0.0",
    "jest": "^27.0.6",
    "jest-config": "^27.0.6",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.4",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.0.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^9.0.0",
    "typedoc": "^0.21.2",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@postman/tunnel-agent": "^0.6.3",
    "agentkeepalive": "^4.1.4",
    "chalk": "^4.1.1",
    "debug": "^4.3.2",
    "mkdirp": "^1.0.4",
    "node-cache": "^5.1.2",
    "node-forge": "^0.10.0",
    "ts-hashcode": "^1.0.2"
  }
}
